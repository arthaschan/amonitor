--T_USER 表，只是为了实验 MYBAITS而用。系统本身不需要使用。

DROP TABLE CDM.T_USER CASCADE CONSTRAINTS;

CREATE TABLE CDM.T_USER
(
  USERNAME  VARCHAR2(10 BYTE),
  PASSWORD  VARCHAR2(10 BYTE),
  AGE       INTEGER,
  NAME      VARCHAR2(10 BYTE),
  ROLE      VARCHAR2(10 BYTE),
  EMAIL     VARCHAR2(10 BYTE),
  PHONE     VARCHAR2(1 BYTE)
)
TABLESPACE TBS_CDM
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING
NOCOMPRESS
NOCACHE
NOPARALLEL
MONITORING;
